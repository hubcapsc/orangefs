 */
static PINT_sm_action setattr_directory_write_distr_dir_data_if_required(
                                                      struct PINT_smcb *smcb,
                                                      job_status_s *js_p)
{
    struct PINT_server_op *s_op = PINT_sm_frame(smcb, PINT_FRAME_CURRENT);
    int ret = 0, dirdata_count = 0;
    job_id_t j_id;
    int i;

    /* reset from jump to here with STATE_DIRDATA or STATE_DIRECTORY */
    js_p->error_code = 0;

    /* if we don't need to write the distr_dir struct, skip it */
    if (!(s_op->req->u.setattr.attr.mask & PVFS_ATTR_DISTDIR_ATTR))
    {
        gossip_debug(GOSSIP_SETATTR_DEBUG,
                     "skipping distr dir struct write\n");
        return SM_ACTION_COMPLETE;
    }

    /* verify that the requested number of servers is sane */
    dirdata_count =
                  s_op->req->u.setattr.attr.u.dir.dist_dir_attr.dirdata_count;
    if ((dirdata_count < 1) ||
        (dirdata_count > PVFS_REQ_LIMIT_DIRENT_FILE_COUNT))
    {
        gossip_err("The requested number of servers (%d) is invalid; "
                   "aborting operation.\n", dirdata_count);
        js_p->error_code = -PVFS_EOVERFLOW;
        return SM_ACTION_COMPLETE;
    }

    /* update fields within the structures that are specific to this server */
    ret = PINT_update_dist_dir_bitmap_from_bitmap(
                &s_op->attr.u.dir.dist_dir_attr,
                s_op->attr.u.dir.dist_dir_bitmap,
                &s_op->req->u.setattr.attr.u.dir.dist_dir_attr,
                s_op->req->u.setattr.attr.u.dir.dist_dir_bitmap);
    if (ret != 0)
    {
        gossip_debug(GOSSIP_SETATTR_DEBUG,
                     "PINT_update_dist_dir_bitmap_from_bitmap error\n");
    }

    /* set up key and value structure for keyval write */
    int32_t keyval_count = 2;

    s_op->key_a = malloc(sizeof(PVFS_ds_keyval) * keyval_count);
    if(!s_op->key_a)
    {
        js_p->error_code = -PVFS_ENOMEM;
        return SM_ACTION_COMPLETE;
    }

    s_op->val_a = malloc(sizeof(PVFS_ds_keyval) * keyval_count);
    if(!s_op->val_a)
    {
        free(s_op->key_a);
        js_p->error_code = -PVFS_ENOMEM;
        return SM_ACTION_COMPLETE;
    }

/* V3 */
#if 0
    s_op->key_a[0].buffer = Trove_Common_Keys[DIST_DIR_ATTR_KEY].key;
    s_op->key_a[0].buffer_sz = Trove_Common_Keys[DIST_DIR_ATTR_KEY].size;

    s_op->val_a[0].buffer = &s_op->attr.u.dir.dist_dir_attr;
    s_op->val_a[0].buffer_sz = sizeof(PVFS_dist_dir_attr);
#endif

    s_op->key_a[0].buffer = Trove_Common_Keys[DIST_DIRDATA_BITMAP_KEY].key;
    s_op->key_a[0].buffer_sz = Trove_Common_Keys[DIST_DIRDATA_BITMAP_KEY].size;

    s_op->val_a[0].buffer = s_op->attr.u.dir.dist_dir_bitmap;
    s_op->val_a[0].buffer_sz = sizeof(PVFS_dist_dir_bitmap_basetype) *
                               s_op->attr.u.dir.dist_dir_attr.bitmap_size;

    s_op->key_a[1].buffer = Trove_Common_Keys[DIST_DIRDATA_HANDLES_KEY].key;
    s_op->key_a[1].buffer_sz = Trove_Common_Keys[DIST_DIRDATA_HANDLES_KEY].size;

    /* TJS: make sure that dirdata_handles gets allocated with contiguous
             SID buffer
    */
    s_op->val_a[1].buffer = s_op->attr.u.dir.dirdata_handles;
    s_op->val_a[1].buffer_sz =
                             OSASZ(s_op->attr.u.dir.dist_dir_attr.dirdata_count,
                                   s_op->attr.u.dir.dist_dir_attr.sid_count);

    for (i = 0; i < s_op->attr.u.dir.dist_dir_attr.dirdata_count; i++)
    {
        if (!PVFS_OID_cmp(&s_op->attr.u.dir.dirdata_handles[i],
                          &s_op->req->u.setattr.handle))
        {
            break;
        }
    }

    if (i < s_op->attr.u.dir.dist_dir_attr.dirdata_count)
    {
        PINT_dist_dir_set_serverno(i,
                                   &s_op->attr.u.dir.dist_dir_attr,
                                   s_op->attr.u.dir.dist_dir_bitmap);
    }
    gossip_debug(GOSSIP_SETATTR_DEBUG,
            " setattr: writing dist-dir-struct keyvals for handle: %s,\n"
            "\t with server_no=%d and branch_level=%d \n",
            PVFS_OID_str(&s_op->req->u.setattr.handle),
            s_op->attr.u.dir.dist_dir_attr.server_no,
            s_op->attr.u.dir.dist_dir_attr.branch_level);

    ret = job_trove_keyval_write_list(s_op->req->u.setattr.fs_id,
                                      s_op->req->u.setattr.handle,
                                      s_op->key_a,
                                      s_op->val_a,
                                      keyval_count,
                                      TROVE_SYNC,
                                      NULL,
                                      smcb,
                                      0,
                                      js_p,
                                      &j_id,
                                      server_job_context,
                                      s_op->req->hints);

    return ret;
}

